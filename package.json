{
  "name": "@imaginelife/nlp-server",
  "version": "0.0.1",
  "description": "An API that serve NLP Content....",
  "main": "index.js",
  "scripts": {
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "local": "nodemon .",
    "local-node": "NODE_ENV=develop ALLOWED_HOST=localhost ALLOWED_QUERY=local-gats nodemon .",
    "start": "NODE_ENV=develop ALLOWED_HOST=$ALLOWED_HOST ALLOWED_QUERY=$ALLOWED_QUERY node .",
    "start-dev": "NODE_ENV=develop ALLOWED_HOST=$ALLOWED_HOST ALLOWED_QUERY=$ALLOWED_QUERY nodemon .",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --runInBand"
  },
  "keywords": [
    "openAI",
    "api",
    "express"
  ],
  "author": "Eric (Jake) Laursen",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "d3-array": "^3.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jest": "^29.7.0",
    "mongodb": "^5.1.0",
    "natural": "^6.2.0",
    "stopword": "^2.0.8",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.3"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 96,
        "branches": 89,
        "functions": 95,
        "lines": 96
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib/index.js",
      "<rootDir>/setup/index.js",
      "<rootDir>/setup/serverKills.js"
    ],
    "globalSetup": "<rootDir>/lib/config/jest-global-setup.js",
    "globalTeardown": "<rootDir>/lib/config/jest-global-teardown.js",
    "setupFilesAfterEnv": [
      "<rootDir>/lib/config/test-env.js"
    ]
  }
}
