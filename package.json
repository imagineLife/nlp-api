{
  "name": "@imaginelife/nlp-server",
  "version": "0.0.1",
  "description": "An API that serve NLP Content....",
  "main": "index.js",
  "scripts": {
    "local": "nodemon .",
    "local-node": "ALLOWED_HOST=localhost ALLOWED_QUERY=local-gats node .",
    "start": "ALLOWED_HOST=$ALLOWED_HOST ALLOWED_QUERY=$ALLOWED_QUERY node .",
    "start-dev": "ALLOWED_HOST=$ALLOWED_HOST ALLOWED_QUERY=$ALLOWED_QUERY nodemon .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --runInBand"
  },
  "keywords": [
    "openAI",
    "api",
    "express"
  ],
  "author": "Eric (Jake) Laursen",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "d3-array": "^3.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "mongodb": "^5.1.0",
    "natural": "^6.2.0",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 74.04,
        "functions": 73.58,
        "lines": 75.25
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib/index.js"
    ],
    "globalSetup": "<rootDir>/lib/config/jest-global-setup.js",
    "globalTeardown": "<rootDir>/lib/config/jest-global-teardown.js",
    "setupFilesAfterEnv": [
      "<rootDir>/lib/config/test-env.js"
    ]
  }
}
